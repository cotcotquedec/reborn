{
  "name": "cotcotquedec/reborn",
  "description": "Reborn from the hashes",
  "keywords": [
    "nofollow",
    "noindex"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Julien Houvion",
      "email": "cotcotquedec@gmail.com",
      "homepage": "https://github.com/cotcotquedec",
      "role": "Big boss"
    }
  ],
  "require": {
    "php": ">=7.1",
    "barryvdh/laravel-debugbar": "^3.1",
    "guzzlehttp/guzzle": "6.*",
    "doctrine/dbal": "^2.6",
    "fideloper/proxy": "~4.0",
    "laravel/framework": "5.6.*",
    "symfony/options-resolver": "~2|~3",
    "almasaeed2010/adminlte": "^2.4.2",
    "webpatser/laravel-uuid": "^3.0",
    "cotcotquedec/frenchfrogs": "dev-master#54bd3b8c8f1b5d957b16531c58eae93e870f3179",
    "jenssegers/date": "^3.2",
    "jenssegers/agent": "^2.3",
    "laravel/socialite": "^3.0",
    "filp/whoops": "^2.1",
    "jdorn/sql-formatter": "^1.2",
    "intervention/image": "^2.3",
    "facebook/graph-sdk": "5.6.*",
    "php-tmdb/laravel": "1.*",
    "predis/predis": "^1.1",
    "roave/better-reflection": "2.*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "ln -sf ../vendor/almasaeed2010/adminlte public/adminlte",
      "ln -sf ../vendor/cotcotquedec/frenchfrogs/assets public/frenchfrogs"
    ],
    "dev": [
      "@composer update",
      "@php artisan ide-helper:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cotcotquedec/frenchfrogs.git"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
