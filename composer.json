{
    "name": "cotcotquedec/reborn",
    "description": "Reborn from the hashes",
    "keywords": ["nofollow", "noindex"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Julien Houvion",
            "email": "cotcotquedec@gmail.com",
            "homepage": "https://github.com/cotcotquedec",
            "role": "Big boss"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
      "laravel/tinker": "~1.0",
        "barryvdh/laravel-ide-helper": "^2.3.2",
        "guzzlehttp/guzzle": "6.*",
        "doctrine/dbal": "^2.5.5",
        "symfony/options-resolver": "~2|~3",
        "barryvdh/laravel-debugbar": "^2.2",
        "almasaeed2010/adminlte": "^2.3",
        "webpatser/laravel-uuid": "2.*",
        "cotcotquedec/frenchfrogs": "dev-1.0-alpha#b9660846c507f5263448fdc0bc730aeebc42fa68",
        "roave/better-reflection": "^1.2",
        "jenssegers/date": "^3.2",
        "jenssegers/agent": "^2.3",
        "laravel/socialite": "^3.0",
        "filp/whoops": "^2.1",
        "jdorn/sql-formatter": "^1.2",
        "intervention/image": "^2.3",
      "facebook/graph-sdk": "^5.4",
      "php-tmdb/laravel": "1.*"
    },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "bootstrap/Ref.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "ln -sf ../vendor/almasaeed2010/adminlte public/adminlte",
            "ln -sf ../vendor/cotcotquedec/frenchfrogs/assets public/frenchfrogs"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize",
            "ln -sf ../vendor/almasaeed2010/adminlte public/adminlte",
            "ln -sf ../vendor/cotcotquedec/frenchfrogs/assets public/frenchfrogs"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cotcotquedec/frenchfrogs"
        }
    ],

    "minimum-stability": "stable",
    "prefer-stable": true,

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
