{
    "name": "cotcotquedec/reborn",
    "description": "Reborn from the hashes",
    "keywords": ["nofollow", "noindex"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "5.1.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/socialite": "^2.0",
        "webpatser/laravel-uuid": "2.*",
        "frenchfrogs/framework": "dev-master#54b56075e80ea51f352508445538636853c546de",
        "frenchfrogs/media" : "dev-master#f354539f384516e59d1a0918cd921b809f150e52"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models",
            "Polliwogs\\": "app/Polliwogs"
        },
        "files": [
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
