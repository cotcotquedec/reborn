{
    "name": "cotcotquedec/reborn",
    "description": "Plateforme Reborn",
    "license": "Copyright",
    "type": "project",
    "authors": [
        {
            "name": "Julien Houvion",
            "email": "cotcotquedec@gmail.com",
            "homepage": "https://github.com/cotcotquedec",
            "role": "Project Manager"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cotcotquedec/php-code-generator",
                "version": "1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/cotcotquedec/php-code-generator.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "5.2.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "guzzlehttp/guzzle": "^6.1",
        "doctrine/dbal": "2.6.*@dev",
        "webpatser/laravel-uuid": "2.*",
        "frenchfrogs/framework": ">=1.8.7",
        "frenchfrogs/acl": "1.*",
        "frenchfrogs/media": "dev-master#f354539f384516e59d1a0918cd921b809f150e52",
        "frenchfrogs/scheduler": "1.*",
        "frenchfrogs/reference": ">=0.1",
        "oyejorge/less.php": "~1.5",
        "matthiasmullie/minify": "^1.3",
        "maknz/slack": "^1.7",
        "league/csv": "^8.0",
        "frenchfrogs/maker": ">=0.1",
        "cotcotquedec/php-code-generator": ">=1",
        "gossi/docblock" : "~1",
        "symfony/options-resolver" : "~2|~3",
        "phootwork/file" : "~0",
        "phootwork/tokenizer" : "~0",
        "nikic/php-parser" : "~1",
        "laravel/socialite": "^2.0",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models",
            "gossi\\codegen\\" : "vendor/cotcotquedec/php-code-generator/src/"
        },
        "files": ["bootstrap/Ref.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
